{
  "name": "the-worlds-most-challenging-puzzle",
  "version": "1.0.0",
  "description": "solve Maze: the world's most challenging puzzle",
  "main": "./lib/main.js",
  "scripts": {
    "start": "node ./lib/main.js",
    "debug": "DEBUG=Maze:* nodemon ./lib/main.js",
    "watch": "npm run jshint && npm run debug",
    "jshint": "jshint ./lib ./tests",
    "test-spec": "tap tests/*_spec.js -R spec",
    "test-perf": "node tests/performance.js",
    "beautify": "js-beautify --end-with-newline --replace ./lib/*.js ./tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonitousconforti/The-worlds-most-challenging-puzzle.git"
  },
  "author": "Leo Conforti",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leonitousconforti/The-worlds-most-challenging-puzzle/issues"
  },
  "homepage": "https://github.com/leonitousconforti/The-worlds-most-challenging-puzzle#readme",
  "devDependencies": {
    "js-beautify": "^1.10.3",
    "jshint": "^2.11.0",
    "nodemon": "^2.0.2",
    "tap": "^14.10.6"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash": "^4.17.15"
  }
}
